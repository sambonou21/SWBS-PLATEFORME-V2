# SWBS-PLATEFORME-V2 - Exemple de configuration .env
# Copiez ce fichier en .env et complétez les valeurs pour votre environnement.

# ---------------------------------------------------
# BASE
# ---------------------------------------------------
NODE_ENV=development
PORT=3000
ABS_PATH_PROJECT=&lt;&lt;ABS_PATH_PROJECT&gt;&gt;
DOMAIN=&lt;&lt;DOMAIN&gt;&gt;

# ---------------------------------------------------
# BASE DE DONNÉES MySQL
# ---------------------------------------------------
DB_HOST=&lt;&lt;DB_HOST&gt;&gt;
DB_PORT=&lt;&lt;DB_PORT&gt;&gt;
DB_NAME=&lt;&lt;DB_NAME&gt;&gt;
DB_USER=&lt;&lt;DB_USER&gt;&gt;
DB_PASS=&lt;&lt;DB_PASS&gt;&gt;

# ---------------------------------------------------
# SESSIONS
# ---------------------------------------------------
SESSION_SECRET=change_this_session_secret
SESSION_NAME=swbs_sid
SESSION_COOKIE_SECURE=false
SESSION_COOKIE_MAX_AGE_DAYS=7

# ---------------------------------------------------
# SMTP / EMAIL
# ---------------------------------------------------
SMTP_HOST=&lt;&lt;SMTP_HOST&gt;&gt;
SMTP_PORT=&lt;&lt;SMTP_PORT&gt;&gt;
SMTP_USER=&lt;&lt;SMTP_USER&gt;&gt;
SMTP_PASS=&lt;&lt;SMTP_PASS&gt;&gt;
MAIL_FROM=&lt;&lt;MAIL_FROM&gt;&gt;

# ---------------------------------------------------
# FEDEPAY
# ---------------------------------------------------
FEDEPAY_PUBLIC=&lt;&lt;FEDEPAY_PUBLIC&gt;&gt;
FEDEPAY_SECRET=&lt;&lt;FEDEPAY_SECRET&gt;&gt;
FEDEPAY_MODE=sandbox

# ---------------------------------------------------
# IA
# ---------------------------------------------------
AI_PROVIDER=&lt;&lt;AI_PROVIDER&gt;&gt;
AI_API_KEY=&lt;&lt;AI_API_KEY&gt;&gt;

# ---------------------------------------------------
# SÉCURITÉ
# ---------------------------------------------------
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=100
CSRF_COOKIE_NAME=swbs_csrf